{
  "expo": {
    "name": "RektefeDV",
    "slug": "rektefe-dv",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "rektefedv",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rektefe.dv",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Uygulama yakındaki ustaları gösterebilmek için konumunuza erişim istiyor.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Uygulama yakındaki ustaları gösterebilmek için konumunuza erişim istiyor.",
        "LSApplicationQueriesSchemes": ["tel", "telprompt", "mailto", "http", "https", "maps"]
      }
    },
    "android": {
      "package": "com.rektefe.dv",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CALL_PHONE"
      ]
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-font"
    ]
  }
}
