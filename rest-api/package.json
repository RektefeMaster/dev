{
  "name": "rest-api",
  "version": "1.0.0",
  "description": "Rektefe REST API",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && npx tsc",
    "start": "node dist/index.js",
    "start:dev": "node -r dotenv/config -r ts-node/register src/index.ts",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config src/index.ts",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "test:e2e": "jest tests/e2e --detectOpenHandles --forceExit --runInBand",
    "test:e2e:watch": "jest tests/e2e --watch --detectOpenHandles --runInBand",
    "render-postbuild": "npm run build",
    "update-mongodb-ip": "node scripts/update-mongodb-ip.js",
    "seed:wash-packages": "ts-node -r dotenv/config src/scripts/seedWashPackages.ts",
    "create:test-users": "ts-node -r dotenv/config scripts/createTestUsers.ts",
    "setup:wash-mechanic": "ts-node -r dotenv/config scripts/setupWashMechanic.ts",
    "clean": "rm -rf dist node_modules/.cache",
    "prebuild": "rm -rf dist"
  },
  "keywords": [
    "api",
    "rest",
    "express",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.8.0",
    "@types/qrcode": "^1.5.5",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.7",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.1",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.10",
    "resend": "^3.0.0",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.9.2",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node-schedule": "^2.1.5",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.2.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node-dev": "^2.0.0"
  }
}
